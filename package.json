{
  "name": "ring-ui-license-checker",
  "version": "1.0.13",
  "description": "Ring UI License Checker is webpack plugin intended to generate third-party libraries list with license information from dependency tree",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "prepublish": "babel src --out-dir lib --source-maps"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "ssh://github.com/JetBrains/ring-ui-license-checker.git"
  },
  "author": "JetBrains",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-polyfill": "6.16.0",
    "nlf": "1.3.1-jb",
    "oss-license-name-to-url": "1.2.1"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.16.0"
  }
}
